    "plugins": [
        "flowtype",
        "compat"
    ],
    "env": {
        "browser": false,
        "jest": true
    },
    "rules": {
        "compat/compat": 2
    },
    "settings": {
        "polyfills": [
            "fetch"
        ]
    }
}